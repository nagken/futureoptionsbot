# IBKR Gateway Credentials - KEEP SECURE
# Copy this file to .env and update with your credentials
# DO NOT commit .env to git!

# IBKR Gateway Login Credentials
IBKR_USERNAME=your_username_here
IBKR_PASSWORD=your_password_here

# Gateway Settings  
IBKR_HOST=127.0.0.1
IBKR_PORT=7496
IBKR_TRADING_MODE=paper

# Monitoring Settings
GATEWAY_CHECK_INTERVAL=30
MAX_RECONNECT_ATTEMPTS=5
RECONNECT_DELAY=10

# Logging
LOG_LEVEL=INFO
LOG_FILE=11_logs/gateway_watchdog.log